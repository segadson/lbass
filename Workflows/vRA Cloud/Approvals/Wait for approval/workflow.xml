    <display-name><![CDATA[Issue created?]]></display-name>
    <script encoded="false"><![CDATA[if (createIssueResult.get("statusCode") == 201) {
 issue = JSON.parse(createIssueResult.get("contentAsString"))
 jiraKey = issue.key
 // Tag provisionied resources with approval JIRA ID 
 attrTags  = inputProperties.get("tags")
 attrTags.put("jiraKey", jiraKey )
 tags = attrTags;
 System.log("Issue created " + jiraKey)
} else {
 System.error("Failed to create JIRA issue! " + createIssueResult.get("contentAsString"))
 throw "Failed to create JIRA issue! " + createIssueResult.get("contentAsString")
}]]></script>
